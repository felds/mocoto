{
  "name": "@felds/mocoto",
  "version": "0.0.1",
  "description": "Open source discord music bot",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon index.ts",
    "build": "rm -fr dist && tsc",
    "start": "ts-node index.ts",
    "format:check": "prettier --check '*.ts' 'src/**/*.ts'",
    "format:fix": "prettier --write '*.ts' 'src/**/*.ts'",
    "lint:check": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix"
  },
  "keywords": [
    "discord",
    "music",
    "bot"
  ],
  "author": "Felds (https://github.com/felds)",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.10.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@discordjs/voice": "^0.6.0",
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0",
    "ffmpeg-static": "^4.4.0",
    "firebase-admin": "^10.0.0",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "ts-node": "^10.4.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.4.3",
    "youtube-scrapper": "^1.1.8",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3"
  }
}
